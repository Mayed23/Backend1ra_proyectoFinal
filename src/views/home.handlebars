{{!-- Productos con Handlebars --}}
<div class ="infoProduct">
        <h1>Lista de Productos con Handlebars</h1>
            <div class="detalleProd">
            <table>
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Modelo</th>
                    <th>Precio</th>
                    <th>Imagen</th>
                    <th>Código</th>
                    <th>Categoría</th>
                    <th>Stock</th>
                    <th>Id</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.model}}</td>
                        <td>{{this.price}}</td>
                        <td><img src="{{this.img}}" alt="Imagen Producto"></td>
                         <td>{{this.code}}</td>
                        <td>{{this.category}}</td>
                        <td>{{this.stock}}</td>      
                        <td>{{this.id}}</td>                
                        
                    </tr>
                {{/each}}
            </tbody>
        </table>
            </div>
        </div>